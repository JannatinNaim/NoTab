{
  "$schema": "http://json-schema.org/draft-07/schema",
  "manifest_version": 3,
  "version": "1.0",
  "name": "No Tab",
  "description": "A clean new tab without distractions.",
  "chrome_url_overrides": {
    "newtab": "clean.html"
  },
  "permissions": ["storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/nothing.png"
  }
}
